{
  "name": "friendlines-backend",
  "version": "1.0.0",
  "description": "Backend server for Friendlines POC - satirical social news app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:reset": "node -e \"require('./utils/devDataInitializer.js').resetDevData().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "test": "jest --passWithNoTests",
    "docs:generate": "node scripts/generate-docs.js"
  },
  "keywords": [
    "express",
    "social",
    "news",
    "api"
  ],
  "author": "Friendlines Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.0",
    "expo-server-sdk": "^3.15.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "firebase-admin": "^13.4.0",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
